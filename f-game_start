def game_start(up, down):
    import random
    
    up, down = int(up), int(down)
    if up > down:
        up, down = down, up
        
    num = random.randint(up, down)
    while True:
        put = input()
        
        if put.isdigit() == False:
            print()
            print('Мы же гадаем целые числа.')
            continue
        
            
        if int(put) > num:     
            print('Число меньше.')
            
        elif int(put) < num:
            print('Число больше.')
            
        elif int(put) == num:
            print()
            print('О да, ты угадал. Это число =', num)
            break
    print()
    return('Сыграем ещё?')
